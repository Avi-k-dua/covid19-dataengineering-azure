{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid19-analysis-azdf"
		},
		"ls_ablob_covid19analysissta_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covid19analysissta'"
		},
		"ls_adls_covid19analysisdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covid19analysisdl'"
		},
		"ls_sqldb_covid19_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_covid19'"
		},
		"ls_adls_covid19analysisdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid19analysisdl.dfs.core.windows.net/"
		},
		"ls_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0831-184947-25b7mlie"
		},
		"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"ls_key_vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://covid19-analysis-kv.vault.azure.net/"
		},
		"tr_population_st_to_dl_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/a99520ae-ae45-4be4-9629-98fe3f487fc0/resourceGroups/covid19-analysis-rg/providers/Microsoft.Storage/storageAccounts/covid19analysissta"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid19-analysis-azdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covid19analysissta_connectionString": {
						"value": "[parameters('ls_ablob_covid19analysissta_connectionString')]"
					},
					"ls_adls_covid19analysisdl_accountKey": {
						"value": "[parameters('ls_adls_covid19analysisdl_accountKey')]"
					},
					"ls_sqldb_covid19_connectionString": {
						"value": "[parameters('ls_sqldb_covid19_connectionString')]"
					},
					"ls_adls_covid19analysisdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covid19analysisdl_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"ls_key_vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_key_vault_properties_typeProperties_baseUrl')]"
					},
					"ls_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid19-analysis-azdf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_st_to_dl_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_st_to_dl_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid19-analysis-azdf_ArmTemplate_0')]"
			]
		}
	]
}